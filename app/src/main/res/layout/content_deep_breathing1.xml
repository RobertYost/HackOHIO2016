<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_deep_breathing1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.robertyost.hackohio2016.DeepBreathing1"
    tools:showIn="@layout/activity_deep_breathing1"
    android:background="?attr/colorPrimary">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

    </RelativeLayout>

    <TextView
        android:text="Deep Breathing Activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:layout_marginTop="27dp"
        tools:fontFamily="sans-serif-smallcaps"
        android:forceHasOverlappingRendering="false"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="36sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:textAlignment="center" />

    <Button
        android:text="Start Breathing Activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="130dp"
        android:id="@+id/btnToDB2"
        android:backgroundTint="@android:color/holo_blue_bright"
        style="@style/Widget.AppCompat.Button.Colored"
        android:visibility="visible"
        android:textAppearance="@style/TextAppearance.AppCompat.Button"
        android:textColorLink="?attr/actionModeSplitBackground"
        android:textColor="@color/colorPrimary"
        android:onClick="SwitchScreens"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />


</RelativeLayout>
